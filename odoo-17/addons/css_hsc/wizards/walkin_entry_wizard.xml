<?xml version="1.0" encoding="utf-8"?>
<odoo>

    

  
    
    <record id="view_walkin_entry_wizard_form" model="ir.ui.view">
        <field name="name">walkin.entry.wizard.form</field>
        <field name="model">walkin.entry.wizard</field>
        <field name="arch" type="xml">
            <form string="Create Entry">
                <group>
                    <group>
                        <field name="walkin_type"  />
                        <field name="employee_ids" widget="many2many_tags" invisible="vehicle_log_id"/> 
                       
                        </group>
                    
                </group>

                <footer>
                    <button string="Submit" type="object" name="submit_vehicle_entry"  class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>  
            </form>
        </field>
        </record>

         <record id="act_walkin_entry_wizard" model="ir.actions.act_window">
        <field name="name">walkin Entry</field>
        <field name="res_model">walkin.entry.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="css_hsc.view_walkin_entry_wizard_form"/>
        <field name="target">new</field>
    </record>



            
</odoo>
